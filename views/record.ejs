<% include ./partials/header %>

    <html>

    <head>
        <style>
            .col-3 {
                height: 30pt;
                font-size: 20px;
            }

            body {
                margin: 0;
            }

            .topnav {
                overflow: hidden;
                background-color: #333;
            }

            .topnav a {
                float: left;
                color: #f2f2f2;
                text-align: center;
                padding: 14px 16px;
                text-decoration: none;
                font-size: 17px;

            }

            .topnav a:hover {
                background-color: #ddd;
                color: black;
            }

            .topnav a.active {
                background-color: #4CAF50;
                color: white;
            }
        </style>
    </head>

    <div class="topnav">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-3">
                    <a href="/welcome">Go Back</a>
                </div>
            </div>
        </div>
    </div>
    <br>
    <br>
    <p> Under Construction</p>
    <div class="col-md-4">
        <%  if(children != null) { %>
            <h2 id="headtag">List of children </h2>
            <table border='1' style="empty-cells:hide" ;width='650' id="recordtable1">
                <thead>
                    <tr>
                        <th class="hidden">ChildID</th>
                        <th>ChildName</th>
                        <th>Gender</th>
                        <th>Phonenumber</th>
                        <th>ChildEmail</th>

                    </tr>
                </thead>

                <% children.forEach(function(record) { %>
                    <tr>
                        <td class="hidden">
                            <%=record.objectId%>
                        </td>
                        <td>
                            <%=record.childFirstName+record.childLastName%>
                        </td>
                        <td>
                            <%=record.childGender%>
                        </td>
                        <td>
                            <%=record.childPhone%>
                        </td>
                        <td>
                            <%=record.childEmail%>
                        </td>

                    </tr>
                    <% }); %>
            </table>
            <% } %>
    </div>

    <div id="latLongTable" class="col-md-8" border='1';width='650' >
        <%  if(location != null) { %>
            <h2 id="headtag">List of Locations </h2>
            <table border='1' style="empty-cells:hide" ;width='650' id="locationTable">
                <thead>
                    <tr>
                        <th class="hidden">LocationId</th>
                        <th>Latitude</th>
                        <th>Longitude</th>
                        <th>Remove</th>                        
                    </tr>
                </thead>

                <% location.forEach(function(record) { %>
                    <tr>
                        <td class="hidden">
                            <%=record.objectId%>
                        </td>
                        <td>
                            <%=record.Latitude%>
                        </td>
                        <td>
                            <%=record.Longitude%>
                        </td>
                        <td>
                            <button>Delete</button>
                        </td>
                    </tr>
                    <% }); %>
            </table>
            <% } %>
    </div>
